# Servidor
PORT=3000
NODE_ENV=development

# RabbitMQ (para Docker)
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASS=guest
RABBITMQ_VHOST=/

# RabbitMQ (para desenvolvimento local)
# RABBITMQ_HOST=localhost
# RABBITMQ_PORT=5692
# RABBITMQ_USER=guest
# RABBITMQ_PASS=guest
# RABBITMQ_VHOST=/

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100

# Baileys Configuration
BAILEYS_SESSION_PATH=./sessions
BAILEYS_AUTH_STRATEGY=local

# JWT (para autenticação opcional)
JWT_SECRET=your-secret-key-here

# Testes
TEST_RABBITMQ_HOST=localhost
TEST_RABBITMQ_PORT=5672 